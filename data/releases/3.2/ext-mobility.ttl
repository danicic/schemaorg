@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfa: <http://www.w3.org/ns/rdfa#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


schema:
  rdf:type owl:Ontology ;
  owl:versionInfo "Created with TopBraid Composer"^^xsd:string ;
.
schema:Accident
  rdf:type owl:Class ;
  schema:refersTo schema:trafficInfoCategory ;
  schema:source "<http://vocab.datex.org/terms/Accident>"^^xsd:string ;
  rdfs:comment "Accidents are events where one or more vehicles are involved in collisions or in leaving the roadway. These include collisions between vehicles or with other road users or obstacles."@en ;
  rdfs:subClassOf schema:TrafficElement ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
schema:Data
  rdf:type owl:Class ;
  schema:source "<http://vocab.datex.org/terms/BasicData>"^^xsd:string ;
  rdfs:comment "Data that is either measured or calculated (elaborated) at the same time or over the same time period."@en ;
  rdfs:subClassOf owl:Thing ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
schema:Destination
  rdf:type owl:Class ;
  schema:source "<http://vocab.datex.org/terms/Destination>"^^xsd:string ;
  rdfs:comment "The specification a destination. This may be either a point location or an area location."@en ;
  rdfs:subClassOf owl:Thing ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
schema:Enumeration
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
.
schema:GeoCoordinates
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
.
schema:GeoShape
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
.
schema:ParkingFacility
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
.
schema:ParkingSite
  rdf:type owl:Class ;
  schema:refersTo schema:parkingSiteMgmtCategory ;
  schema:source "<http://vocab.datex.org/terms/ParkingSite>"^^xsd:string ;
  rdfs:comment "A record containing static details of a parking site. Must be specialised as an 'Urban-' or 'InterUrbanParkingSite' or a 'SpecialLocationParkingSite'."@en ;
  rdfs:subClassOf owl:Thing ;
  owl:equivalentClass schema:ParkingFacility ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
schema:ParkingSiteAddress
  rdf:type owl:ObjectProperty ;
  rdfs:domain schema:ParkingSite ;
  rdfs:range schema:PostalAddress ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
schema:ParkingSiteStatus
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
schema:ParkingSpace
  rdf:type owl:Class ;
  schema:refersTo schema:parkingSpaceMgmtCategory ;
  schema:source "<http://vocab.datex.org/terms/ParkingSpace>"^^xsd:string ;
  rdfs:comment "A single parking space. It is possible to define the same parking space more than once with different properties, e.g. when there is a different parking assignment for different times."@en ;
  rdfs:subClassOf owl:Thing ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
schema:ParkingSpaceStatus
  rdf:type owl:Class ;
  schema:source "<http://vocab.datex.org/terms/ParkingSpaceStatus>"^^xsd:string ;
  rdfs:comment "Status (occupied or closed) for a single parking space which was defined in the static part of the model."@en ;
  rdfs:subClassOf owl:Thing ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
schema:Place
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
  schema:isPartOf <http://mobility.schema.org> ;
.
schema:Pollution
  rdf:type owl:Class ;
  schema:refersTo schema:environment ;
  schema:refersTo schema:trafficInfoCategory ;
  schema:source "<http://vocab.datex.org/terms/Pollution>"^^xsd:string ;
  rdfs:comment "Details of atmospheric pollution."@en ;
  rdfs:subClassOf owl:Thing ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
schema:PostalAddress
  rdf:type owl:Class ;
  schema:source "<http://vocab.datex.org/terms/Contact>"^^xsd:string ;
  rdfs:comment "Address and contact information about some person, service or the parking site, provided in detail or via reference."@en ;
  rdfs:subClassOf owl:Thing ;
.
schema:TrafficData
  rdf:type owl:Class ;
  schema:refersTo schema:trafficInfoCategory ;
  schema:source "<http://vocab.datex.org/terms/TrafficData>"^^xsd:string ;
  rdfs:comment "Measured or derived values relating to traffic or individual vehicle movements on a specific section or at a specific point on the road network."@en ;
  rdfs:subClassOf schema:Data ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
schema:TrafficElement
  rdf:type owl:Class ;
  schema:source "<http://vocab.datex.org/terms/TrafficElement>"^^xsd:string ;
  rdfs:comment "An event which is not planned by the traffic operator, which is affecting, or has the potential to affect traffic flow."@en ;
  rdfs:subClassOf owl:Thing ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
schema:TrafficSpeed
  rdf:type owl:Class ;
  schema:source "<http://vocab.datex.org/terms/TrafficSpeed>"^^xsd:string ;
  rdfs:comment "Averaged measurements or calculations of traffic speed."@en ;
  rdfs:subClassOf schema:TrafficData ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
schema:TravelTimeData
  rdf:type owl:Class ;
  schema:refersTo schema:trafficInfoCategory ;
  schema:source "<http://vocab.datex.org/terms/TravelTimeData>"^^xsd:string ;
  rdfs:comment "Derived/computed travel time information relating to a linear section of the road network; forecast = true means a forecast for a vehicle at the start of the specified location, forecast = false means calculation/measurement at the end."@en ;
  rdfs:subClassOf schema:Data ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
schema:accidentType
  rdf:type owl:DatatypeProperty ;
  rdfs:domain schema:Accident ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
schema:address
  rdf:type owl:ObjectProperty ;
  rdfs:range schema:PostalAddress ;
.
schema:busTransportation
  rdf:type schema:Enumeration ;
  schema:supersededBy "schema:transportation"^^xsd:string ;
  rdfs:comment "A sub-category of tranasporation offerings which relate to bus transportation"^^xsd:string ;
  schema:isPartOf <http://mobility.schema.org> ;
.
schema:carTransportation
  rdf:type schema:Enumeration ;
  schema:supersededBy "schema:transportation"^^xsd:string ;
  rdfs:comment "A sub-category of tranasporation offerings which relate to car transportation"^^xsd:string ;
  schema:isPartOf <http://mobility.schema.org> ;
.
schema:environment
  rdf:type schema:Enumeration ;
  schema:source "<http://sensormeasurement.appspot.com/m3/Environment>"^^xsd:string ;
  rdfs:comment "A category for all the Offerings relating to environment"^^xsd:string ;
  schema:isPartOf <http://mobility.schema.org> ;
.
schema:latitude
  rdf:type owl:DatatypeProperty ;
  rdfs:domain schema:GeoCoordinates ;
  rdfs:range xsd:decimal ;
  rdfs:range xsd:string ;
.
schema:location
  rdf:type owl:ObjectProperty ;
.
schema:longitude
  rdf:type owl:DatatypeProperty ;
  rdfs:range xsd:decimal ;
  rdfs:range xsd:string ;
.
schema:offeringCategory
  rdf:type schema:Enumeration ;
  schema:supersededBy "schema:parking"^^xsd:string ;
  schema:supersededBy "schema:traffic"^^xsd:string ;
  schema:supersededBy "schema:transportation"^^xsd:string ;
  rdfs:comment "Categorization of the offerings"^^xsd:string ;
  schema:isPartOf <http://mobility.schema.org> ;
.
schema:parking
  rdf:type schema:Enumeration ;
  rdfs:comment "A category for all the Offerings relating to parking"^^xsd:string ;
  schema:isPartOf <http://mobility.schema.org> ;
.
schema:parkingDescription
  rdf:type owl:ObjectProperty ;
  schema:isPartOf <http://mobility.schema.org> ;
.
schema:parkingLocation
  rdf:type owl:ObjectProperty ;
  rdfs:domain schema:ParkingSpace ;
  rdfs:range schema:GeoCoordinates ;
  schema:isPartOf <http://mobility.schema.org> ;
.
schema:parkingName
  rdf:type owl:DatatypeProperty ;
  rdfs:domain schema:ParkingSite ;
  rdfs:domain schema:ParkingSpace ;
  rdfs:range xsd:string ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
schema:parkingNumberOfOccupiedSpaces
  rdf:type owl:DatatypeProperty ;
  schema:source "<http://vocab.datex.org/terms/parkingNumberOfOccupiedSpaces>"^^xsd:string ;
  rdfs:domain schema:ParkingSpaceStatus ;
  rdfs:label "parking number of occupied spaces"^^xsd:string ;
  rdfs:range xsd:integer ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
schema:parkingNumberOfVacantSpaces
  rdf:type owl:DatatypeProperty ;
  schema:source "<http://vocab.datex.org/terms/parkingNumberOfVacantSpaces>"^^xsd:string ;
  rdfs:domain schema:ParkingSiteStatus ;
  rdfs:label "parking number of vacant spaces"^^xsd:string ;
  rdfs:range xsd:integer ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
schema:parkingSiteMgmtCategory
  rdf:type schema:Enumeration ;
  schema:supersededBy "schema:parking"^^xsd:string ;
  rdfs:comment "Offerings relating to Parking site can be categorized under ParkingSiteCategory"@en ;
  schema:isPartOf <http://mobility.schema.org> ;
.
schema:parkingSiteStatus
  rdf:type owl:DatatypeProperty ;
  rdfs:domain schema:ParkingSite ;
  rdfs:range xsd:boolean ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
schema:parkingSpaceMgmtCategory
  rdf:type schema:Enumeration ;
  schema:supersededBy "schema:parking"^^xsd:string ;
  rdfs:comment "A sub-category of Parking, offerings relating to parking space can be categorized under ParkingSpaceCategory"@en ;
  schema:isPartOf <http://mobility.schema.org> ;
.
schema:parkingSpaceOrGroupIdentifier
  rdf:type owl:ObjectProperty ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
schema:parkingSpaceStatus
  rdf:type owl:ObjectProperty ;
  rdfs:domain schema:ParkingSpace ;
  rdfs:range schema:ParkingSpaceStatus ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
schema:publicationTime
  rdf:type owl:DatatypeProperty ;
  schema:source "<http://vocab.datex.org/terms/publicationTime>"^^xsd:string ;
  rdfs:range xsd:dateTime ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
schema:refersTo
  rdf:type owl:ObjectProperty ;
  schema:isPartOf <http://mobility.schema.org> ;
.
schema:source
  rdf:type owl:DatatypeProperty ;
.
schema:supersededBy
  rdf:type rdf:Property ;
.
schema:totalCapacity
  rdf:type owl:DatatypeProperty ;
  schema:source "<http://vocab.datex.org/terms/totalCapacity>"^^xsd:string ;
  rdfs:domain schema:ParkingSite ;
  rdfs:range xsd:integer ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
schema:traffic
  rdf:type schema:Enumeration ;
  rdfs:comment "A category for all the Offerings relating to traffic"^^xsd:string ;
  schema:isPartOf <http://mobility.schema.org> ;
.
schema:trafficInfoCategory
  rdf:type schema:Enumeration ;
  schema:supersededBy "schema:traffic"^^xsd:string ;
  rdfs:comment "A sub-category of traffic. All offerings relating to  traffic information can be categorized under TrafficInfoCategory"^^xsd:string ;
  schema:isPartOf <http://mobility.schema.org> ;
.
schema:trafficNotificationsCategory
  rdf:type schema:Enumeration ;
  schema:supersededBy "schema:traffic"^^xsd:string ;
  schema:isPartOf <http://mobility.schema.org> ;
.
schema:transportation
  rdf:type schema:Enumeration ;
  schema:source "<http://sensormeasurement.appspot.com/m3/Transportation>"^^xsd:string ;
  rdfs:comment "A category for all the Offerings relating to transportation"^^xsd:string ;
  schema:isPartOf <http://mobility.schema.org> ;
.
schema:vehicleInvolved
  rdf:type owl:ObjectProperty ;
  rdfs:domain schema:Accident ;
  schema:isPartOf <http://mobility.schema.org> ;
  dct:source <http://vocab.datex.org/terms> ;
.
